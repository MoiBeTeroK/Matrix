#include <iostream>
using namespace std;

void main() {
    int n; cin >> n;
    float** a;
    a = new float* [n];
    for (int i = 0; i < n; i++) {
        a[i] = new float[n];
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cin >> a[i][j];
        }
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cour << a[i][j] << "\t";
        }
        cput << endl;
    }
    for (int i = 0; i < n; i++) {
        delete a[i];
    }
    delete[]a;
}
